{
  "name": "mobx-keys-store",
  "version": "1.1.0",
  "description": "Keys store for MobX",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "ntsc --outDir ./dist --experimentalDecorators --target es5 --module commonjs --declaration --pretty src/index.ts",
    "test": "NODE_ENV=test nyc mocha",
    "lint": "tslint './src/**/*.ts'",
    "watch": "nodemon -e .ts -i node_modules -i dist -i coverage -x mocha",
    "docs": "typedoc --mode file --theme minimal --tsconfig ./tsconfig.json --excludePrivate --out ./docs ./src",
    "docs:add": "npm run docs && git add docs/",
    "dist:add": "npm run build && git add dist/",
    "precommit": "npm run lint && npm test && npm run dist:add && npm run docs:add"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/mobx-keys-store.git"
  },
  "keywords": [
    "mobx",
    "keys",
    "store",
    "observable"
  ],
  "bugs": {
    "url": "https://github.com/infinum/mobx-keys-store/issues"
  },
  "homepage": "https://github.com/infinum/mobx-keys-store#readme",
  "author": "Infinum <javascript.team@infinum.co>",
  "contributors": [
    {
      "name": "Darko Kukovec",
      "email": "darko@infinum.co"
    }
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/interfaces/*",
      "test/*"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.40",
    "@types/node": "^9.3.0",
    "chai": "^4.0.0",
    "husky": "^0.14.0",
    "mobx": "^4.1.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.11.0",
    "ntypescript": "^1.201609302242.1",
    "nyc": "^11.0.2",
    "ts-node": "^5.0.0",
    "tslint": "^5.2.0",
    "typedoc": "^0.11.0",
    "typescript": "^2.3.0"
  },
  "peerDependencies": {
    "mobx": "^2.7.0 || ^3"
  }
}
